<ul class="list-group">
  {{#each comentarios}}
  <div class="mb-3">
    <div class="form-group" id="comentarios">
      <h4 class="list-group-item-heading">{{user}} | Puntaje: {{puntaje}}
      </h4>
      <p class="list-group-item-text">{{comentario}}</p>
       <button type="submit" value="{{id_comentario}}" id="borrarComentario" class="borrarComentario">Eliminar</button>
    </div>
    <div class="form-group">
    </div>
  </div>
  {{/each}}
</ul>
